#sxhkd

#browser
alt + c
      chromium

#terminal
alt + shift + Return
      urxvt

#powermenu
alt + Escape
      powermenu 

#screenshot
alt + Insert
      sleep 1 && scrot -s -e 'feh $f' 

#increase volume
alt + Page_Up
      amixer sset Master 5%+
#decrease volume
alt + Page_Down
      amixer sset Master 5%-
#mute
alt + Delete 
      {amixer sset Master mute, amixer sset Master unmute}

#dmenu clipboard
alt + o 
      clipmenu -i -fn spleen:size=20
